extends layout
  
block content

  // Preloader
  div#preloader
    div#status

  // Home start
  section#homeyouthcouncil2015.pfblock-image.screen-height
    .home-overlay
    .intro
      .start= t('home.start')
      a(href='#training2015')= t('home.achievement')
      .start= t('home.startDescription')
    a(href='#training2015')
      .scroll-down
        span
          i.fa.fa-angle-down.fa-2x
  // Home end

  // Navigation start
  header.header
    nav.navbar.navbar-custom(role="navigation")
      .container
        .navbar-header
          button.navbar-toggle(type="button", data-toggle="collapse", data-target="#custom-collapse")
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href="/")= t('home.start')
        #custom-collapse.collapse.navbar-collapse
          ul.nav.navbar-nav.navbar-right
            li
              a(href="#home")= t('nav.home')
            li
              a(href="#youthcouncil2015")= t('nav.youthcouncil2015') 
            li
              a(href="#youthcouncil2015")= t('nav.youthcouncil2015') 
            li
              a(href="#teams")= t('nav.teams') 
            li
              a(href="#partners")= t('nav.partners') 
            li
              a(href="#contact")= t('nav.contact') 
            li
              a.aFlag.dropdown-toggle(type='button', data-toggle='dropdown')
                span.fa.fa-flag
                span= t('nav.languages')
                span.caret
              ul.dropdown-menu(role='menu')
                li
                  a(href='?setLng=ar')= t('nav.arabe') 
                li.divider
                li
                  a(href='?setLng=en')= t('nav.english') 
                li.divider
                li
                  a(href='?setLng=fr')= t('nav.french') 
                li.divider
                li
                  a(href='?setLng=dev')= t('nav.test') 
    // .container
  // Navigation end

  // training & workshop 2015 starts: description + gallery
  section#trainingWorkshop2015.pfblock
    .container
      .row
        .col-sm-6.col-sm-offset-3
          .pfblock-header.wow.fadeInUp
            h2.pfblock-title= t('youthcouncil2013.training2013')
            .pfblock-line
            .pfblock-subtitle= t('youthcouncil2013.figCaptionDescription1')
      // The Gallery as lightbox dialog, should be a child element of the document body
      .container
        // The Bootstrap Image Gallery lightbox, should be a child element of the document body
        #blueimp-gallery.blueimp-gallery.blueimp-gallery-controls
          // The container for the modal slides
          .slides
          // Controls for the borderless lightbox
          h3.title
          a.prev &#8249;
          a.next &#8250;
          a.close &#215;
          a.play-pause
          ol.indicator
          // The modal dialog, which will be used to wrap the lightbox content
          .modal.fade
            .modal-dialog
              .modal-content
                .modal-header
                  button.close(type="button", aria-hidden="true") &#215;
                  h4.modal-title
                .modal-body.next
                .modal-footer
                  button.btn.btn-primary.pull-left.prev(type="button")
                    i.fa.fa-arrow-left
                    | Previous
                  button.btn.btn-primary.next(type="button")
                    | Next
                    i.fa.fa-arrow-right
     
        #linkstrainingPictures
